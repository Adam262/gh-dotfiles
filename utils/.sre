n2ip () {
    local text profile
    text="$1"
    profile=${2:-"dev.use1"}

	aws ec2 --profile="$profile" describe-instances --filters "Name=tag:Name,Values=*${text}*" Name=instance-state-name,Values=running | jq -r '.Reservations[].Instances[] | [.NetworkInterfaces[0].PrivateIpAddress, (.Tags[] | select(.Key == "Name").Value),(.InstanceId),(.LaunchTime)] | join("\t")'
}

get-asgs () {
    local profile
    profile=${1:-"dev.use1"}
    
	aws autoscaling --profile="$profile" describe-auto-scaling-groups | jq -er '.AutoScalingGroups[].AutoScalingGroupName'
}

asgterm () {
    local instance-id profile
    instance-id="$1"
    profile=${2:-"dev.use1"}

    aws autoscaling terminate-instance-in-auto-scaling-group --no-should-decrement-desired-capacity --instance-id "$instance-id"
}
